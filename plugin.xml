<?xml version="1.0" encoding="UTF-8"?>
<!-- The plugin.dtd file is located in the OXYGEN_INSATALL_DIR/plugins directory -->
<!DOCTYPE plugin SYSTEM "../plugin.dtd">

<plugin
  id="sample-plugin"
  name="Sample Oxygen XML Editor Plugin"
  description="Un plugin simplu de Oxygen care inverseaza Y cu Z cand se tasteaza."
    version="1.0"
    vendor="Syncro Soft">
  <runtime>
    <librariesFolder name="lib" />
    <library name="target/classes" />
    <librariesFolder name="target/lib" />
  </runtime>
  <extension type="WorkspaceAccess"
            class="com.oxygenxml.sample.WorkspaceAccessImpl"/>
</plugin>
